<Window x:Class="DCS_Nexus.SlaveDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Detail Window" Height="200" Width="300">
    <Grid>
        <TextBlock Text="ID:" Margin="10"/>
        <TextBlock Text="{Binding ID}" Margin="50,10,10,10"/>
        <TextBlock Text="Mac:" Margin="10,30,10,10"/>
        <TextBlock Text="{Binding MacUpper}" Margin="50,30,10,10"/>
    </Grid>
</Window>
